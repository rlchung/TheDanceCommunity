<!DOCTYPE html>
    <head>
        <link rel= "stylesheet" href="../../stylesheets/local.css"/>
        <title>Dance Community Events near <%= nearby.baseCity %> | TheDanceCommunity</title>
    </head>
    <body>
        <h2><%= nearby.baseCity%></h2>
        <h2> Nearby teams: </h2>
        
        <div class="content">
            <div class="container">
                <section class="featured">
                    <ul class="grid">
                        <% teams.forEach(function(team){ %>
                            <li>
                                <img src= "<%= team.profilePic%>">
                            </li>
                        <% }) %>
                    </ul>
                </section>
            </div>
        </div>
            
    </body>
</html>